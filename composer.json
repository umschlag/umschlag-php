{
    "name": "umschlag/umschlag",
    "type": "library",
    "description": "docker distribution management system",
    "keywords": ["docker", "registry"],
    "homepage": "https://umschlag.webhippie.de",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Thomas Boerger",
            "email": "thomas@webhippie.de"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Umschlag\\": "lib/Umschlag"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umschlag\\Tests\\": "tests/Umschlag/Tests"
        }
    },
    "scripts": {
        "post-install-cmd": ["@composer bin all install --ansi"],
        "post-update-cmd": ["@composer bin all update --ansi"]
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false
        }
    }
}
