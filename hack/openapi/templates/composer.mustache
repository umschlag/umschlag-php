{
    "name": "{{gitUserId}}/{{packageName}}",
    "type": "library",
    {{#artifactVersion}}
    "version": "{{artifactVersion}}",
    {{/artifactVersion}}
    "description": "{{{appDescription}}}",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "docker",
        "registry"
    ],
    "homepage": "https://github.com/{{gitUserId}}/{{gitRepoId}}",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Thomas Boerger",
            "email": "thomas@webhippie.de"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12",
        "phan/phan": "~1.3",
        "phpstan/phpstan": "~0.11",
        "codacy/coverage": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "{{escapedInvokerPackage}}\\" : "{{srcBasePath}}/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{escapedInvokerPackage}}\\" : "{{testBasePath}}/"
        }
    }
}
